<odoo>
    <record id="view_call_history_tree" model="ir.ui.view">
        <field name="name">webrtc.call.history.tree</field>
        <field name="model">webrtc.call.history</field>
        <field name="arch" type="xml">
            <tree string="Historique des appels">
                <field name="caller"/>
                <field name="callee"/>
                <field name="call_duration"/>
                <field name="call_status"/>
            </tree>
        </field>
    </record>
</odoo>
