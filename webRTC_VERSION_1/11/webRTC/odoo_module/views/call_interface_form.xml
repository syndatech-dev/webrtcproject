<odoo>
    <record id="view_call_interface_form" model="ir.ui.view">
        <field name="name">webrtc.call.history.form</field>
        <field name="model">webrtc.call.history</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="user_id"/>
                    <field name="call_date"/>
                    <field name="call_duration"/>
                    <field name="status"/>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
